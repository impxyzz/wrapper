{
	"compilerOptions": {
		"target": "esnext",
		"module": "ESNext",
		"moduleResolution": "Classic",
		"rootDir": "../../../",
		"baseUrl": "../../../",
		"noEmit": true,
		"noUnusedLocals": true,
		"strict": true,
		"alwaysStrict": true,
		"experimentalDecorators": true
	},
	"exclude": [
		"wrapper_autogen/**/*",
		"node_modules/**/*"
	],
	"compileOnSave": true
}