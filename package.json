{
	"name": "fusion-native2-scripts",
	"version": "1.0.0",
	"description": "Fusion Native 2: Scripts",
	"devDependencies": {
		"@babel/cli": "7.4.4",
		"@babel/core": "7.4.5",
		"@babel/plugin-syntax-bigint": "7.4.4",
		"@babel/preset-typescript": "7.3.3",
		"tslint": "next",
		"typescript": "next"
	},
	"scripts": {
		"watch": "npx babel --watch src_ts --extensions \".ts\" --out-dir scripts",
		"watch_bootstrap": "npx babel --watch src_bootstrap --extensions \".ts\" --out-file bootstrap.js",
		"build": "npx babel src_ts --extensions \".ts\" --out-dir scripts",
		"build_bootstrap": "npx babel src_bootstrap --extensions \".ts\" --out-file bootstrap.js",
		"check-types": "npx tsc",
		"lint": "npx tslint --fix 'src_*/**/*.ts'"
	},
	"author": "moofMonkey"
}
