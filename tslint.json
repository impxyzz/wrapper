{
	"defaultSeverity": "error",
	"extends": [
		"tslint:latest"
	],
	"jsRules": {},
	"rules": {
		"semicolon": [
			false,
			"never"
		],
		"no-trailing-whitespace": true,
		"whitespace": [
			true,
			"check-preblock",
			"check-module"
		],
		"indent": [
			true,
			"tabs",
			1
		],
		"curly": false,
		"arrow-parens": [
			true,
			"ban-single-arg-parens"
		],
		"max-line-length": false,
		"object-literal-sort-keys": false,
		"one-variable-per-declaration": false,
		"no-var-keyword": false,
		"no-console": false,
		"no-bitwise": false,
		"radix": false,
		"prefer-const": false,
		"variable-name": [
			"check-format",
			"allow-trailing-underscore",
			"allow-pascal-case",
			"allow-snake-case",
			"ban-keywords"
		],
		"member-access": true,
		"no-string-throw": false,
		"max-classes-per-file": false,
		"class-name": false,
		"unified-signatures": false,
		"one-line": [
			true,
			"check-catch",
			"check-finally",
			"check-else",
			"check-open-brace",
			"check-whitespace"
		],
		"member-ordering": [
			true,
			{
				"order": "statics-first",
				"alphabetize": false
			}
		],
		"comment-format": false,
		"no-implicit-dependencies": false,
		"no-submodule-imports": false
	},
	"rulesDirectory": [],
	"linterOptions": {
		"exclude": [
			"**/node_modules/**"
		]
	}
}